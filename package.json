{
  "name": "pb-math-service",
  "version": "2.0.0",
  "description": "Modern math rendering service with MathJax 4, SVG/PNG generation, and speech text support",
  "main": "app.js",
  "scripts": {
    "start": "node bin/www",
    "dev": "NODE_ENV=development nodemon bin/www",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.js\""
  },
  "keywords": [
    "mathjax",
    "latex",
    "mathml",
    "asciimath",
    "svg",
    "png",
    "accessibility",
    "speech"
  ],
  "author": "Pressbooks",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@mathjax/mathjax-mhchem-font-extension": "^4.0.0",
    "@mathjax/mathjax-newcm-font": "^4.0.0",
    "@mathjax/src": "^4.0.0",
    "mhchemparser": "^4.2.1",
    "@resvg/resvg-js": "^2.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "lru-cache": "^10.1.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "optionalDependencies": {
    "@vendia/serverless-express": "^4.12.6"
  }
}
